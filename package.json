{
  "name": "init-vite-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "build:web": "npm run lint && npm run build",
    "build:dev": "./node_modules/.bin/env-cmd -f .env.dev -- npm run build:web",
    "build:stage": "./node_modules/.bin/env-cmd -f .env.stage -- npm run build:web",
    "build:production": "./node_modules/.bin/env-cmd -f .env.prod -- npm run build:web",
    "dev": "npm run lingui:extract:compile && ./node_modules/.bin/env-cmd -f .env.dev -- vite",
    "preview": "npm run build:dev && vite preview",
    "lint": "npm run lint:biome && npm run lint:ts && npm run lint:css && npm run lingui:extract:compile:strict",
    "lint:biome": "biome check . && biome lint --error-on-warnings .",
    "lint:css": "stylelint **/*.css",
    "lint:ts": "tsc -p tsconfig.json --noEmit",
    "lingui:extract:compile": "lingui extract --clean && lingui compile",
    "lingui:extract:compile:strict": "lingui extract --clean && lingui compile --strict",
    "biome:format": "biome format .",
    "css:format": "stylelint --fix",
    "preinstall:supported:browsers": "chmod +x scripts/generateSupportedBrowsers.sh",
    "supported:browsers": "./scripts/generateSupportedBrowsers.sh"
  },
  "dependencies": {
    "@generouted/react-router": "1.20.0",
    "@lingui/core": "5.6.1",
    "@lingui/react": "5.6.1",
    "axios": "1.13.2",
    "classnames": "2.5.1",
    "js-cookie": "3.0.5",
    "logger-browser": "git+https://git.cwi.com.br/stacks/typescript/packages/logger-browser#v1.1.1",
    "react": "19.2.0",
    "react-dialogs": "git+https://git.cwi.com.br/stacks/react/packages/react-dialogs#v1.3.1",
    "react-dom": "19.2.0",
    "react-hooks": "git+https://git.cwi.com.br/stacks/react/packages/react-hooks#v1.1.0",
    "react-router": "7.9.5",
    "react-router-dom": "7.9.5",
    "react-use": "17.6.0",
    "utils": "git+https://git.cwi.com.br/stacks/typescript/packages/utils#v1.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.4",
    "@lingui/babel-plugin-lingui-macro": "5.6.1",
    "@lingui/vite-plugin": "5.6.1",
    "@types/node": "24.10.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "5.1.0",
    "babel-plugin-macros": "3.1.0",
    "babel-plugin-react-compiler": "1.0.0",
    "browserslist-useragent-regexp": "4.1.3",
    "env-cmd": "11.0.0",
    "sharp": "0.34.5",
    "stylelint": "16.26.1",
    "stylelint-config-clean-order": "8.0.0",
    "svgo": "4.0.0",
    "typescript": "5.9.3",
    "vite": "7.2.2",
    "vite-imagetools": "9.0.0",
    "vite-plugin-image-optimizer": "2.0.3",
    "vite-plugin-pwa": "1.2.0",
    "vite-plugin-svgr": "4.5.0"
  }
}
